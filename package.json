{
  "name": "@inksha/toolsets",
  "version": "1.0.0",
  "description": "My collection of commonly used development tools.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "toJs": "tsc",
    "format": "prettier --write ./src/**",
    "doc": "typedoc --options typedoc.json",
    "dev:build": "rollup -c rollup.config.ts --configPlugin typescript --watch",
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "push": "npm run format && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:InkSha/toolset.git"
  },
  "author": "InkSha",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.18",
    "@types/multer": "^1.4.9",
    "@types/mysql": "^2.15.22",
    "@types/node": "^20.8.2",
    "@types/sharp": "^0.32.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typedoc": "^0.25.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemon": "^3.0.1",
    "sharp": "^0.32.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
